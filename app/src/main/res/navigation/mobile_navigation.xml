<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.nfd.dicodingeventapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.nfd.dicodingeventapp.ui.upcoming.UpComingFragment"
        android:label="Event Up Coming"
        tools:layout="@layout/fragment_up_coming" />
    <fragment
        android:id="@+id/navigation_finished"
        android:name="com.nfd.dicodingeventapp.ui.finished.FinishedFragment"
        android:label="Event Finished"
        tools:layout="@layout/fragment_finished" />
</navigation>